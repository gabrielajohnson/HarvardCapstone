{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    },
    {
      "src": "../frontend/src/main.js",
      "use": "@vercel/static",
      "config": { "includeFiles": ["../frontend/dist/**"] }
    }
  ],
  "rewrites": [
    {
      "source": "./routes/api/(.*)",
      "destination": "./server.js"
    },
    {
      "source": "../frontend/src/main.js",
      "destination": "../frontend/dist/index.html"
    },
  ]
}
